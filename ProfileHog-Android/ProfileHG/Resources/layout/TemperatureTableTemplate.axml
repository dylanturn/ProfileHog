                            p1:layout_column="2"
                            p1:id="@+id/textView13"
                            p1:layout_marginLeft="5dp" />
                        <TextView
                            p1:text="Average"
                            p1:textAppearance="?android:attr/textAppearanceSmall"
                            p1:id="@+id/textView14"
                            p1:layout_marginLeft="5dp" />
