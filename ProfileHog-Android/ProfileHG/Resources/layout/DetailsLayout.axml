<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/DetailsLayout"
    android:background="#ffffff"
    android:alpha=".75">
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/DetailsScroll">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/DetailsDetails" />
    </ScrollView>
    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/performanceCounterTable"
        android:layout_weight=".20"
        android:padding="5dp"
        android:layout_gravity="center"
        android:alpha="1">
        <TableRow
            android:id="@+id/counterTitleRow"
            android:alpha="1"
            android:gravity="center">
            <TextView xmlns:tools="http://schemas.android.com/tools"
                android:text="Counter"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_column="0"
                android:id="@+id/textView39"
                tools:text="Counter"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView xmlns:tools="http://schemas.android.com/tools"
                android:text="Current"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_column="1"
                android:id="@+id/textView40"
                tools:text="Current"
                android:minWidth="75dp"
                android:layout_gravity="center"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView xmlns:tools="http://schemas.android.com/tools"
                android:text="Highest"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_column="2"
                android:id="@+id/textView46"
                tools:text="Highest"
                android:layout_gravity="center"
                android:minWidth="75dp"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView xmlns:p2="http://schemas.android.com/tools"
                android:text="Average"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_column="3"
                android:id="@+id/textView41"
                p2:text="Average"
                android:layout_gravity="center"
                android:minWidth="75dp"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
        </TableRow>
        <TableRow
            android:id="@+id/cpuTableRow"
            android:gravity="center">
            <TextView xmlns:p7="http://schemas.android.com/tools"
                android:text="CPU Usage"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_column="0"
                android:id="@+id/cpuTitle"
                p7:text="CPU Usage"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView xmlns:tools="http://schemas.android.com/tools"
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="1"
                android:id="@+id/cpuCurrent"
                android:gravity="center"
                android:layout_gravity="center"
                android:numeric="integer"
                android:textColor="@android:color/white" />
            <TextView xmlns:tools="http://schemas.android.com/tools"
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="2"
                android:id="@+id/cpuHighest"
                android:gravity="center"
                android:textColor="@android:color/white" />
            <TextView xmlns:tools="http://schemas.android.com/tools"
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="3"
                android:id="@+id/cpuAverage"
                android:gravity="center"
                android:textColor="@android:color/white" />
        </TableRow>
        <TableRow
            android:id="@+id/ramTableRow"
            android:gravity="center">
            <TextView xmlns:p7="http://schemas.android.com/tools"
                android:text="Used Ram (MB)"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_column="0"
                android:id="@+id/ramTitle"
                p7:text="Available Ram"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="1"
                android:id="@+id/ramCurrent"
                android:gravity="center"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="2"
                android:id="@+id/ramHighest"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="3"
                android:id="@+id/ramAverage"
                android:textColor="@android:color/white" />
        </TableRow>
        <TableRow
            android:id="@+id/diskReadTableRow"
            android:gravity="center">
            <TextView xmlns:p7="http://schemas.android.com/tools"
                android:text="Disk Reads"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_column="0"
                android:id="@+id/diskReadTitle"
                p7:text="Disk Reads"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView
                android:text="0"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="1"
                android:id="@+id/diskReadCurrent"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="2"
                android:id="@+id/diskReadHighest"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="3"
                android:id="@+id/diskReadAverage"
                android:textColor="@android:color/white" />
        </TableRow>
        <TableRow
            android:id="@+id/diskWriteTableRow"
            android:gravity="center">
            <TextView xmlns:p8="http://schemas.android.com/tools"
                android:text="Disk Writes"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_column="0"
                android:id="@+id/diskWriteTitle"
                p8:text="Disk Writes"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView
                android:text="0"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="1"
                android:id="@+id/diskWriteCurrent"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="2"
                android:id="@+id/diskWriteHighest"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:gravity="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="3"
                android:id="@+id/diskWriteAverage"
                android:textColor="@android:color/white" />
        </TableRow>
        <TableRow
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/gpuCoreTableRow"
            android:gravity="center">
            <TextView
                android:text="GPU Core (%)"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/gpuCoreTitle"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/gpuCoreCurrent"
                android:layout_gravity="center"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/gpuCoreHighest"
                android:layout_gravity="center"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/gpuCoreAverage"
                android:layout_gravity="center"
                android:textColor="@android:color/white" />
        </TableRow>
        <TableRow
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/gpuMemTableRow"
            android:gravity="center">
            <TextView
                android:text="GPU MEM (%)"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/gpuMemoryTitle"
                android:textColor="@android:color/white"
                android:textSize="17.5dp"
                android:typeface="sans"
                android:textStyle="normal" />
            <TextView
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/gpuMemoryCurrent"
                android:layout_gravity="center"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/gpuMemoryHighest"
                android:layout_gravity="center"
                android:textColor="@android:color/white" />
            <TextView
                android:text="0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/gpuMemoryAverage"
                android:layout_gravity="center"
                android:textColor="@android:color/white" />
        </TableRow>
    </TableLayout>
</LinearLayout>